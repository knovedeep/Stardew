		{
			"LogName": "Zinnia Default",
			"Action": "EditImage",
			"Target": "Portraits/Zinnia, Characters/Zinnia, Portraits/Zinnia_Beach, Characters/Zinnia_Beach",
			"FromFile": "AGECompat/{{Target}}.png",
			"When":
			{
				"HasFile:{{FromFile}}": true,
				"AdventurersGuildExpandedCompatibility |contains= AllModded, ModdedZinniaOnly": true
			}
		},
