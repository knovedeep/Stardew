	"ConfigSchema":
	{		
		"FlowerQueensCrown":
		{
			"AllowValues": "Random, Everyone",
			"Default": "Random",
			"Description": "Gives other NPCs a chance to be Flower Queen during the Flower Dance. If 'random is selected, one person will be chosen each year, while if 'everyone' is selected, all eligible townspeople will wear Flower Queen crowns.",
		},
		"TownspeopleOnly":
		{
			"AllowValues": "true, false",
			"Default": "false",
			"Description": "Allows custom NPCs to be Flower Queen candidates as well. Set to 'true' if you only want the Pelican Town marriage candidates and children to be eligible.",
		},
		
// CHARACTER VARIANTS
		
		"AbigailVariant":
		{
			"AllowValues": "VanillaStraightSize, VanillaPlusSize, ModdedStraightSize, ModdedPlusSize, off",
			"Default": "VanillaStraightSize",
		},
		"AlexVariant":
		{
			"AllowValues": "Vanilla, Mexican, Samoan, off",
			"Default": "Vanilla",
		},
		"ElliottVariant":
		{
			"AllowValues": "Vanilla, Modded, off",
			"Default": "Vanilla",
		},
		"EmilyVariant":
		{
			"AllowValues": "Vanilla, Black, Romani, off",
			"Default": "Vanilla",
		},
		"HaleyVariant":
		{
			"AllowValues": "Vanilla, Black, Romani, off",
			"Default": "Vanilla",
		},
		"HarveyVariant":
		{
			"AllowValues": "Vanilla, ModdedNonSikh, ModdedSikh, off",
			"Default": "Vanilla",
		},
		"JasVariant":
		{
			"AllowValues": "Vanilla, Modded, off",
			"Default": "Vanilla",
		},
		"LeahVariant":
		{
			"AllowValues": "Vanilla, Native, Butch, off",
			"Default": "Vanilla",
		},
		"MaruVariant":
		{
			"AllowValues": "Vanilla, ModdedLavender, ModdedNotsnufffie, off",
			"Default": "Vanilla",
		},
		"PennyVariant":
		{
			"AllowValues": "Vanilla, ModdedAirynNotsnufffieLarge, ModdedAirynNotsnufffieSmall, Mixed, off",
			"Default": "Vanilla",
		},
		"SamVariant":
		{
			"AllowValues": "Vanilla, ModdedLighter, ModdedDarker, off",
			"Default": "Vanilla",
		},
		"SebastianVariant":
		{
			"AllowValues": "Vanilla, ModdedBlack, ModdedPurple, off",
			"Default": "Vanilla",
		},
		"ShaneVariant":
		{
			"AllowValues": "Vanilla, Modded, off",
			"Default": "Vanilla",
		},
		"VincentVariant":
		{
			"AllowValues": "Vanilla, Modded, off",
			"Default": "Vanilla",
		},
		
// INDIVIDUAL OPTIONS
		"EmilyTattoos":
		{
			"AllowValues": "Tattoos, NoTattoos",
			"Default": "Tattoos",
			"Description": "Chooses whether or not Emily has colorful tattoos.",
		},
		"HaleyAccessories":
		{
			"AllowValues": "Piercings, Cuffs",
			"AllowBlank": true,
			"AllowMultiple": true,
			"Description": "Gives Haley nose and eyebrow piercings, and/or gives her hair cuffs. Hair cuffs only apply to Haley's Black variant.",
		},
		"HarveyCharacterMustache":
		{
			"AllowValues": "Mustache, NoMustache",
			"Default": "NoMustache",
			"Description": "Gives Harvey's character/overworld sprite a mustache to match his portraits. Only affects the Vanilla variant.",
		},
		"MaruCharacterGlasses":
		{
			"AllowValues": "Glasses, NoGlasses",
			"Default": "NoGlasses",
			"Description": "Gives Maru's character/overworld sprite glasses to match her portraits. Only affects the Vanilla and ModdedNotsnufffie variants.",
		},
		"SamEyes":
		{
			"AllowValues": "Default, Alternate, Heterochromia",
			"Default": "Default",
			"Description": "Keeps Sam's default eye color, changes it to an alternate color, or gives him heterochromia (each eye is a different color).",
		},
		"SamExtras":
		{
			"AllowValues": "Stubble, Beard, Piercings",
			"AllowBlank": true,
			"AllowMultiple": true,
			"Description": "Gives Sam stubble, a beard, and/or lip and eyebrow piercings.",
		},
		"SebGlasses":
		{
			"AllowValues": "Glasses, NoGlasses",
			"Default": "NoGlasses",
			"Description": "Gives Sebastian glasses. ('Dynamic' option is not available in FQC).",
		},
		"SebPiercings":
		{
			"AllowValues": "Piercings, NoPiercings",
			"Default": "NoPiercings",
			"Description": "Gives Sebastian lip and ear piercings.",
		},
	},
